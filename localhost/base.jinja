<!DOCTYPE html>{% set STATIC = url_for('static', filename='').split('?')[0] %}
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="{% block description %}{% endblock %}">
    <title>{{ config['TITLE'] }}</title>
    <meta name="author" content="{{ config['AUTHOR'] }}">
    <link href="{{ STATIC }}favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <link href="{{ STATIC }}{{ config['TEMPLATE'] }}/css/bootstrap.css" rel="stylesheet" type="text/css">
    <link href="{{ STATIC }}{{ config['TEMPLATE'] }}/css/bootstrap-responsive.css" rel="stylesheet" type="text/css">
    <link href="{{ STATIC }}{{ config['TEMPLATE'] }}/css/custom.css" rel="stylesheet" type="text/css">
    <script src="{{ STATIC }}{{ config['TEMPLATE'] }}/js/jquery-1.7.2.min.js" type="text/javascript"></script>
    <script src="{{ STATIC }}{{ config['TEMPLATE'] }}/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="{{ STATIC }}{{ config['TEMPLATE'] }}/js/bootstrap-tooltip.js" type="text/javascript"></script>
    {% block head %}{% endblock %}
</head>
<body>
<div class="container">
    {% block main %}{% endblock %}
</div>
{% block jquery %}{% endblock %}
</body>
